  "dimension_descriptions": {
    "openness": {
      "name": "Openness to Experience",
      "high": "Imaginative, curious, open to new experiences, artistic, intellectually curious",
      "low": "Practical, conventional, prefers routine, traditional"
    },
    "conscientiousness": {
      "name": "Conscientiousness", 
      "high": "Organized, responsible, dependable, persistent, achievement-oriented",
      "low": "Spontaneous, flexible, careless, disorganized"
    },
    "extraversion": {
      "name": "Extraversion",
      "high": "Sociable, assertive, energetic, enthusiastic, talkative",
      "low": "Reserved, quiet, independent, prefers solitude"
    },
    "agreeableness": {
      "name": "Agreeableness",
      "high": "Cooperative, trusting, helpful, compassionate, kind",
      "low": "Competitive, skeptical, independent, direct"
    },
    "neuroticism": {
      "name": "Neuroticism",
      "high": "Anxious, moody, emotionally unstable, sensitive to stress",
      "low": "Calm, emotionally stable, resilient, confident"
    }
  },
  "relationship_implications": {
    "openness": {
      "high": "Values growth, change, and new experiences in relationships. May seek partners who challenge them intellectually.",
      "low": "Prefers stable, predictable relationships. Values tradition and shared routines."
    },
    "conscientiousness": {
      "high": "Reliable partner, follows through on commitments. May have high expectations for organization.",
      "low": "Spontaneous and flexible. May struggle with consistency in relationship maintenance."
    },
    "extraversion": {
      "high": "Seeks social connection, enjoys group activities. Needs social stimulation in relationships.",
      "low": "Values deep one-on-one connections. Prefers intimate settings over social gatherings."
    },
    "agreeableness": {
      "high": "Cooperative and accommodating. May struggle with asserting needs in relationships.",
      "low": "Direct and honest in communication. May need to work on empathy and compromise."
    },
    "neuroticism": {
      "high": "May need extra emotional support. Benefits from patient, understanding partners.",
      "low": "Emotionally stable foundation for relationships. Provides calm presence during stress."
    }
  }
